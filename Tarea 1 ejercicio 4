#include <iostream>
#include <math.h>
using namespace std ;
int main() {
    int option ;
    float x1, x2, y1, y2, distancia ;
    cout<< "ingrese x1: " ;
    cin>> x1 ;
    cout<< "ingrese x2: " ;
    cin>> x2 ;
    cout<< "ingrese y1: " ;
    cin>> y1 ;
    cout<< "ingrese y2: " ;
    cin>> y2 ;
    distancia=sqrt(pow ((x2-x1),2)+pow ((y2-y1),2));
    cout<< "la distancia dado dos puntos es: "<< distancia;
}
